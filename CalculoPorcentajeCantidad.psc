Algoritmo CalculoPorcentajeCantidad
	Escribir "Digite el número que necesita calcular el porcentaje"
	Leer valorP
	Escribir "Digite el valor del procentaje que desea calcular"
	Leer porcentaje
	resultado <- valorP*(porcentaje/100)
	Escribir "El " porcentaje "% de " valorP " es " resultado
FinAlgoritmo
